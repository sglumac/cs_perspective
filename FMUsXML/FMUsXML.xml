                <Parameter name="MDL.EngineModel_2_R2018b.w_omega.v" value="1."/>
                <Parameter name="MDL.EngineModel_2_R2018b.w_alpha.v" value="1."/>
            </Parameters>
        </Instance>
		<Instance instanceName="INERTIA">
            <Archive archiveName="simulationModel_2.fmu"/>
            <Parameters>
                <Parameter name="MDL.simulationModel_2.J.v" value="10."/>
                <Parameter name="MDL.simulationModel_2.d.v" value="10."/>
