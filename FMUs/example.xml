<?xml version="1.0"?>
<Configuration xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <Instances>
        <Instance instanceName="Alpha">
            <Archive archiveName="simpleStep.fmu"/>
            <Parameters>
                <Parameter name="MDL.simpleStep.stepFinalValue.v" value="1."/>
                <Parameter name="MDL.simpleStep.stepStartValue.v" value="0."/>
                <Parameter name="MDL.simpleStep.stepTime.v" value="1."/>
            </Parameters>
        </Instance>
        <Instance instanceName="Engine">
            <Archive archiveName="EngineModel_2_R2018b.fmu"/>
            <Parameters>
                <Parameter name="MDL.EngineModel_2_R2018b.w_omega.v" value="1."/>
                <Parameter name="MDL.EngineModel_2_R2018b.w_alpha.v" value="1."/>
            </Parameters>
        </Instance>
		<Instance instanceName="Inertia">
            <Archive archiveName="simulationModel_2.fmu"/>
            <Parameters>
                <Parameter name="MDL.simulationModel_2.J.v" value="10."/>
                <Parameter name="MDL.simulationModel_2.d.v" value="10."/>
            </Parameters>
        </Instance>
    </Instances>
    <Connections>
        <Connection>
            <Source instanceName="Alpha" outputName="output"/>
            <Destination instanceName="Engine" inputName="alpha"/>
        </Connection>
        <Connection>
            <Source instanceName="Engine" outputName="torque"/>
            <Destination instanceName="Inertia" inputName="torque"/>
        </Connection>
		<Connection>
            <Source instanceName="Inertia" outputName="velocity"/>
            <Destination instanceName="Engine" inputName="velocity"/>
        </Connection>
    </Connections>
</Configuration>